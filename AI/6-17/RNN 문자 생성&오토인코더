oded = layers.Dense(784, activation='sigmoid')(encoded)\n","\n","# 입력을 재구성에 매핑\n","autoencoder = keras.Model(input_img, decoded)"],"metadata":{"id":"bXuB23vXhrbc","executionInfo":{"status":"ok","timestamp":1750066893904,"user_tz":-540,"elapsed":9290,"user":{"displayName":"박인철","userId":"06375623913430966996"}}},"execution_count":22,"outputs":[]},{"cell_type":"code","source":["# This model maps an input to its encoded representation\n","encoder = keras.Model(input_img, encoded)"],"metadata":{"id":"QhFP3SP9h5b0"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 입력을 인코딩된 표현에 매핑\n","encoded_input = keras.Input(shape=(encoding_dim,))\n","\n","# 오토인코더 모델의 마지막 레이어를 검색\n","decoder_layer = autoencoder.layers[-1]\n","\n","# 디코더 모델 생성\n","decoder = keras.Model(encoded_input, decoder_layer(encoded_input))"],"metadata":{"id":"ywrrA1Izh8PU","executionInfo":{"status":"ok","timestamp":1750066958753,"user_tz":-540,"elapsed":7,"user":{"displayName":"박인철","userId":"06375623913430966996"}}},"execution_count":23,"outputs":[]},{"cell_type":"code","source":["autoencoder.compile(optimizer='adam', loss='binary_crossentropy')"],"metadata":{"id":"W2t41sVhh-1k","executionInfo":{"status":"ok","timestamp":1750066962328,"user_tz":-540,"elapsed":27,"user":{"displayName":"박인철","userId":"06375623913430966996"}}},"execution_count":24,"outputs":[]},{"cell_type":"code","source":["from keras.datasets import mnist\n","import numpy as np\n","(x_train, _), (x_test, _) = mnist.load_data()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"bfS_QvcYiBRc","executionInfo":{"status":"ok","timestamp":1750066964870,"user_tz":-540,"elapsed":609,"user":{"displayName":"박인철","userId":"06375623913430966996"}},"outputId":"2c562f5c-57ae-4b36-c3ae-8c88a728fa42"},"execution_count":25,"outputs":[{"output_type":"stream","name":"stdout","text":["Downloading data from https://storage.googleapis.com/tensorflow/tf-keras-datasets/mnist.npz\n","\u001b[1m11490434/11490434\u001b[0m \u001b[32m━━━━